<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <rewrite>
            <rules>
                <rule name="ReverseProxyInboundRule1" stopProcessing="true">
                    <match url="(.*)" />
                    <conditions>
                        <add input="{REQUEST_URI}" pattern="^/dashboard" negate="true" />
                    </conditions>
                    <action type="Rewrite" url="http://localhost:5005/{R:1}" />
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
